<%= form_tag sessions_path do %>
  <%= content_tag(:div) do %>
    <%= label_tag :email %>
    <%= email_field_tag :email%>
  <% end %>
  <%= content_tag(:div) do %>
    <%= label_tag :password %>
    <%= password_field_tag :password%>
  <% end %>
  <%= content_tag(:div) do %>
    <%= submit_tag "Log in" %>
  <% end %>
<% end %>
